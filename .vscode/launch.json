{
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Program",
            "type": "node",
            "program": "${workspaceFolder}/src/index.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
            "env": {
                "SEQUELIZE_URL": "postgres://postgres:abc123@db-dev:5432/postgres"
            },
            "attachSimplePort": 9229
        }
    ]
}